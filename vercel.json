{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/server.js"
    },
    {
      "src": "/login",
      "dest": "/server.js"
    },
    {
      "src": "/signup",
      "dest": "/server.js"
    },
    {
      "src": "/home",
      "dest": "/server.js"
    },
    {
      "src": "/admin",
      "dest": "/server.js"
    },
    {
      "src": "/newuser",
      "dest": "/server.js"
    },
    {
      "src": "/logout",
      "dest": "/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/html/(.*)",
      "dest": "/public/html/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "/public/favicon.ico"
    },
    {
      "src": "/apple-touch-icon.png",
      "dest": "/public/apple-touch-icon.png"
    },
    {
      "src": "/favicon-16x16.png",
      "dest": "/public/favicon-16x16.png"
    },
    {
      "src": "/favicon-32x32.png",
      "dest": "/public/favicon-32x32.png"
    },
    {
      "src": "/favicon-new.png",
      "dest": "/public/favicon-new.png"
    },
    {
      "src": "/favicon-new-16.png",
      "dest": "/public/favicon-new-16.png"
    },
    {
      "src": "/icon-192x192.png",
      "dest": "/public/icon-192x192.png"
    },
    {
      "src": "/icon-512x512.png",
      "dest": "/public/icon-512x512.png"
    },
    {
      "src": "/site.webmanifest",
      "dest": "/public/site.webmanifest"
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server.js": {
      "maxDuration": 30
    }
  }
}